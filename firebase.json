{
  "hosting": {
    "public": "dist/thicongchuc.org",
    "headers": [{
      "source": "**",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=20"
      }]
    }],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}
