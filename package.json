{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "ng": "ng",
    "dev": "ng serve -o",
    "start": "ng serve",
    "start:es": "ng serve -c=es --port 4201",
    "build": "ng build",
    "build:prod": "set NODE_ENV=production&& ng build --prod --build-optimizer --aot --stats-json",
    "build:i18n": "set NODE_ENV=production&& ng build --prod --build-optimizer --aot --stats-json --localize",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "ng extract-i18n --output-path locale --ivy",
    "analyze": "webpack-bundle-analyzer dist/angular-boilerplate/stats.json"
  },
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "@angular/service-worker": "~11.2.8",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.8",
    "@angular/localize": "^11.2.8",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@cypress/webpack-preprocessor": "5.7.0",
    "@types/jasmine": "~3.6.9",
    "@types/jest": "26.0.22",
    "@types/node": "^14.14.37",    
    "@typescript-eslint/parser": "4.21.0",
    "@typescript-eslint/eslint-plugin": "4.21.0",    
    "autoprefixer": "^10.2.5",
    "cypress": "7.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-import": "2.22.1",    
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "jest": "26.6.3",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "tailwindcss": "^2.1.1",
    "ts-loader": "8.1.0",
    "ts-node": "~9.1.1",
    "typescript": "^4.1.5",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
